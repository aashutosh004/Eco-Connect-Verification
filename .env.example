ENV=production
LOG_LEVEL=INFO
CORS_ORIGINS=https://your-frontend.example.com

# InsightFace
INSIGHT_PACK=buffalo_l
DET_W=416
DET_H=416

# Thresholds (calibrate!)
EMBEDDING_THRESHOLD=0.62
LIVENESS_THRESHOLD=0.70

# Liveness
LIVENESS_ENABLED=true
LIVENESS_IMG_SIZE=80
LIVENESS_NUM_CLASSES=3
REAL_CLASS_INDEX=1

# Models
YOLO_MODEL_PATH=models/best.pt
LIVENESS_MODEL_PTH=models/2.7_80x80_MiniFASNetV2.pth

# Debug
DEBUG_ROUTES=false
